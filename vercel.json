{
  "rewrites": [
    {
      "source": "/civilization/:path*",
      "destination": "https://civilization-[YOUR-PROJECT-NAME].vercel.app/:path*"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_LINT_DURING_BUILD": "false"
  }
}
